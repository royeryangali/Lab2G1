<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.edu.pucp.tel306">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_panda"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_panda_round"
        android:supportsRtl="true"
        android:theme="@style/porDefecto">
        <activity android:name=".tareas_pendientes" android:configChanges="uiMode"></activity>
        <activity android:name=".agregar_tarea" android:parentActivityName=".tareas_pendientes" ></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>